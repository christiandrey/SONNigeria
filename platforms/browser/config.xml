<?xml version='1.0' encoding='utf-8'?>
<widget id="com.devvv.sonnigeria" version="1.1.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>SON(Nigeria)</name>
    <description>
        Information App for Standards Organisation of Nigeria
    </description>
    <author email="oluwaseun.adedire@gmail.com" href="http://www.gmail.com">
        Oluwaseun Adedire
    </author>
    <content src="index.html" />
    <icon src="icon.png" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="android-minSdkVersion" value="14" />
</widget>
